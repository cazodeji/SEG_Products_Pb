  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "dev": "AWS_PROFILE=seg-dev nodemon -e ts  --exec \"sls offline\"",
    "windev": "cmd /c \"set AWS_PROFILE=seg-dev&& npx nodemon -e ts --exec \"sls offline\"\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint '*/**/*.{js,ts}' ",
    "lint:fix": "eslint '*/**/*.{js,ts}' --fix ",
    "--dev--": "--------------------------------------------------------",
    "package-dev": "AWS_PROFILE=seg-dev sls package",
    "deploy-dev": "AWS_PROFILE=seg-dev sls deploy",
    "package-windev": "cmd /c \"set AWS_PROFILE=seg-dev&& sls package\"",
    "deploy-windev": "cmd /c \"set AWS_PROFILE=seg-dev&& sls deploy\""
